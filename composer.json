{
    "name": "c975l/countlinescode-bundle",
    "type": "symfony-bundle",
    "description": "This bundle counts the lines of code written in a Symfony project",
    "keywords": ["count", "code", "symfony", "bundle"],
    "homepage": "https://github.com/975L/CountLinesCodeBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/LaurentMarquet",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": "^7",
        "c975l/config-bundle": "^2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0",
        "phpunit/phpunit": "^7",
        "rector/rector": "^0",
        "symplify/easy-coding-standard": "^5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c975L\\CountLinesCodeBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/CountLinesCodeBundle",
        "issues": "https://github.com/975L/CountLinesCodeBundle/issues"
    }
}
